<div class="header__second-row side-pads">
  <div class="header-container">
    <div class='menu-container'>
      <div id="menu-toggle">
        <i class="fas fa-bars"></i><span>MENU</span>
      </div>
      <ul id='menu' class="header__menu">
        {% for link in section.settings.menu.links%}
        <li><a href="{{link.url | url}}">{{link.title | escape}}</a></li>
        {% endfor %}
      </ul>
    </div>
    <div class='authentication'>
      <a href="" class="header__sign-up-btn">
        <span >{%if customer%}Sign Out{%else%}Sign In{%endif%}</span>
      </a>   
    </div>
  </div>
</div>

